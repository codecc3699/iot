void setup()
{
Serial.begin(9600);
Serial.println("led is bliking");
pinMode(4,OUTPUT);
pinMode(5,OUTPUT);
}
void loop()
{
digitalWrite(4,HIGH);
delay(500);
Serial.println("LED 2 is blinking\n");
digitalWrite(4,LOW);
delay(500);
Serial.println("LED 2 is OFF\n");
digitalWrite(5,HIGH);
delay(500);
Serial.println("LED 3 is blinking\n");
digitalWrite(4,LOW);
delay(500);
Serial.println("LED 3 is OFF\n");
}
